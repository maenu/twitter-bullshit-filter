{
	"name": "Twitter Bullshit Filter",
	"version": "0.1.0",
	"description": "Filters bullshit from your twitter timeline",
	"permissions": [
		"activeTab"
	],
	"icons": {
		"16": "logo/logo-16x16.png",
		"32": "logo/logo-32x32.png",
		"48": "logo/logo-48x48.png",
		"128": "logo/logo-128x128.png"
	},
	"content_scripts": [
		{
			"matches": [
				"https://twitter.com/*"
			],
			"css": [
				"content.css"
			],
			"js": [
				"jquery-3.3.1.min.js",
				"content.js"
			]
		}
	],
	"manifest_version": 2
}